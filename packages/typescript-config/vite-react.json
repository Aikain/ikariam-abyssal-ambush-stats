{
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "sourceMap": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "jsx": "react-jsx"
    },
    "exclude": ["node_modules"]
}
